{
	"training_data": "./data/",
  	"output": "./output/results/",
  	"dataset_division": "271_5fold",

  	"embedding_module": "AttrE",
  	"alignment_module": "sharing",
	"search_module": "greedy",
	  
    "dim": 200,
  	"init": "normal",
	"ent_l2_norm": true,
	"rel_l2_norm": true,
	"attr_l2_norm": true,
	"char_l2_norm": true,
	"loss_norm": "L2",
  	"margin": 1.5,
	"loss": "margin-based",

    "neg_sampling": "uniform",
	"neg_triple_num": 1,

	"learning_rate": 0.01,
    "optimizer": "SGD",
	"max_epoch": 2000,
	"batch_size": 200,

	"batch_threads_num": 1,
	"test_threads_num": 2,

	"ordered": true,
	"start_valid": 100,
	"eval_freq": 10,
	"stop_metric": "hits1",
	"eval_metric": "inner",
	"csls": 10,
	"top_k": [1, 5, 10, 50],
	"is_save": true,
	"eval_norm": true,
	"literal_len": 5
}
