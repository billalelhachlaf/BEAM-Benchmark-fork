{
	"training_data": "./data/",
  	"output": "./output/results/",
  	"dataset_division": "271_5fold",

  	"embedding_module": "BootEA",
  	"alignment_module": "swapping",
	"search_module": "greedy",
	  
    "dim": 100,
  	"init": "normal",
	"ent_l2_norm": true,
	"rel_l2_norm": true,
	"loss": "limited",
	"loss_norm": "L2",
	  
	"learning_rate": 0.01,
    "optimizer": "Adagrad",
	"max_epoch": 2000,
	"batch_size": 100,

	"pos_margin": 0.5,
  	"neg_margin": 5.0,
    "neg_margin_balance": 1,

  	"neg_sampling": "truncated",
	"neg_triple_num": 10,
	"truncated_epsilon": 0.9,
	"truncated_freq": 10,

	"batch_threads_num": 1,
	"test_threads_num": 2,

	"ordered": true,
	"start_valid": 100,
	"eval_freq": 10,
	"stop_metric": "mrr",
	"eval_metric": "inner",
	"csls": 10,
	"top_k": [1, 5, 10, 50],
	"is_save": true,
	"eval_norm": true,

	"sim_th": 0.7,
	"k": 10,

	"likelihood_slice": 10,
	"sub_epoch": 10
}
